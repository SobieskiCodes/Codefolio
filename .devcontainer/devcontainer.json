{
	"name": "Class Work Display App",
	"features": {
		"github-cli": "latest"
	},
	"dockerComposeFile": "../docker-compose.yml",
	"service": "backend",
	"workspaceFolder": "/workspaces/CodefolioHomeworkDisplay",
	"forwardPorts": [3000, 8000],
	"portsAttributes": {"3000": {"label": "React App"}, "8000": {"label": "FastAPI"}},
	"postCreateCommand": "python .devcontainer/set_ports_to_public.py $CODESPACE_NAME",
	"shutdownAction": "stopCompose",
	"customizations": {
	  "vscode": {
		"settings": {
		  "terminal.integrated.profiles.linux": {
			"bash": {
			  "path": "bash"
			}
		  },
		  "terminal.integrated.defaultProfile.linux": "bash"
		},
		"extensions": [
		  "dbaeumer.vscode-eslint"
		]
	  }
	}
  }
  