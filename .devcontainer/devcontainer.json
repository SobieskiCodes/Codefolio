{
	"name": "Class Work Display App",
	"features": {
		"github-cli": "latest"
	},
	"dockerComposeFile": "../docker-compose.yml",
	"service": "backend",
	"workspaceFolder": "/",
	"forwardPorts": [3000, 8000],
	"portsAttributes": {"3000": {"label": "React App"}, "8000": {"label": "FastAPI"}},
	"shutdownAction": "stopCompose",
	"onCreateCommand": "python ./backend/presetup.py $CODESPACE_NAME $GITHUB_TOKEN",
	"postCreateCommand": "gh codespace ports visibility 3000:'public' -c $CODESPACE_NAME && gh codespace ports visibility 8000:'public' -c $CODESPACE_NAME",
	"customizations": {
	  "vscode": {
		"settings": {
		  "terminal.integrated.profiles.linux": {
			"bash": {
			  "path": "bash"
			}
		  },
		  "terminal.integrated.defaultProfile.linux": "bash"
		},
		"extensions": [
		  "dbaeumer.vscode-eslint"
		]
	  }
	}
  }
  